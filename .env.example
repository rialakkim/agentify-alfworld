# OpenAI API Key (required for white agent)
OPENAI_API_KEY=your-api-key-here

# White Agent Configuration
WHITE_AGENT_MODEL=gpt-4o-mini
WHITE_AGENT_PROVIDER=openai
WHITE_AGENT_TEMPERATURE=0.0

# Enhanced reasoning mode for white agent (true/false)
# When enabled, the white agent tracks state and avoids repetition
WHITE_AGENT_REASONING=true

# ALFWorld Data Path (optional, defaults to ~/.cache/alfworld/)
# ALFWORLD_DATA=/path/to/alfworld/data
